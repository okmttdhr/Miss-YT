{
  "name": "YoutuVote",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js run-ios",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm i",
    "lint": "$(npm bin)/eslint ./",
    "lint:fix": "yarn lint -- --fix",
    "test": "yarn lint && yarn test:unit",
    "test:unit": "NODE_ENV=production $(npm bin)/ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "tron": "node_modules/.bin/reactotron",
    "flow": "flow --show-all-errors"
  },
  "dependencies": {
    "apisauce": "^0.6.0",
    "bluebird": "^3.4.7",
    "firebase": "^4.1.2",
    "format-json": "^1.0.3",
    "lodash": "^4.17.2",
    "nock": "^9.0.4",
    "querystringify": "0.0.4",
    "ramda": "^0.22.1",
    "react": "16.0.0-alpha.6",
    "react-native": "0.43.3",
    "react-native-animatable": "^0.6.0",
    "react-native-config": "^0.4.2",
    "react-native-device-info": "^0.9.6",
    "react-native-drawer": "^2.3.0",
    "react-native-i18n": "^0.1.1",
    "react-native-maps": "^0.11.0",
    "react-native-router-flux": "3.38.0",
    "react-native-vector-icons": "^4.0.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-persist": "^3.5.0",
    "redux-saga": "^0.13.0",
    "reduxsauce": "0.2.0",
    "seamless-immutable": "^6.3.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "ava-spec": "^1.1.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "0.40.0",
    "ghooks": "^1.3.2",
    "mockery": "^2.0.0",
    "nyc": "^9.0.1",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.4.0",
    "react-native-mock": "^0.2.8",
    "reactotron-apisauce": "^1.5.2",
    "reactotron-react-native": "^1.5.2",
    "reactotron-redux": "^1.5.2",
    "reactotron-redux-saga": "^1.5.2",
    "sinon": "^2.3.5",
    "snazzy": "^5.0.0"
  },
  "ava": {
    "files": [
      "App/{,**/}*.spec.js",
      "Tests/**/*.js",
      "!Tests/Setup.js",
      "!Tests/mock/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill",
      "react-native-mock/mock",
      "./Tests/Setup"
    ],
    "babel": "inherit"
  },
  "config": {
    "ghooks": {
      "pre-commit": "if [ -d 'ignite-base' ]; then cd ignite-base; fi; yarn test"
    }
  }
}
